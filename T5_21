#include <iostream>

using namespace std;

int main()
{
    int num,a,b,c,d;
    cin>>num;
    switch(num)
    {
    case 1:
        cout<<"请领取固定工资"<<endl;
        cin>>a;
        break;
    case 2:
        cout<<"请输入本周工作时间"<<endl;
        cin>>b;
        break;
    case 3:
        cout<<"请输入本周销售毛利"<<endl;
        cin>>c;
        break;
    case 4:
        cout<<"请输入本周生产产品件数"<<endl;
        cin>>d;
        break;
    }
    return 0;
}
