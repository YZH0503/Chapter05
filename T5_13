#include <iostream>

using namespace std;

int main()
{
    int x;
    cin>>x;
    for(int y=0;y<5;y++)
    {
        for(int i=0;i<x;i++)
        {
            cout<<"*";
        }
        cout<<"\n";
        cin>>x;
    }
    return 0;
}
