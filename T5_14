#include <iostream>

using namespace std;

int main()
{
    int x;
    double y;
    cin>>x>>y;
    switch(x)
    {
        case 1:cout<<2.98*y;break;
        case 2:cout<<4.50*y;break;
        case 3:cout<<9.98*y;break;
        case 4:cout<<4.49*y;break;
        case 5:cout<<6.87*y;break;
    }
    return 0;
}
