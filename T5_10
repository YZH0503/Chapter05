#include<iostream>

using namespace std;

int main()
{
	int i, j;//¼ÆÊýÆ÷
	int a;//½×³Ë
	for (i = 1; i <= 5;i++)
	{
		a = 1;
		for (j = i;j > 0;j--)
		{
			a = a * j;
		}
		cout << i << "!=" << a << endl;
	}
	return 0;
}
